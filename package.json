{
    "name": "vega-preview",
    "displayName": "Vega Preview",
    "description": "Preview for Vega and Vega-Lite",
    "version": "0.9.0",
    "publisher": "mdk",
    "repository": {
        "url": "https://github.com/8/vega-preview"
    },
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vega-preview.showVegaPreview",
        "onCommand:vega-preview.showVegaLitePreview",
        "onCommand:vega-preview.exportVegaToSvg",
        "onCommand:vega-preview.exportVegaLiteToSvg"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "vega-preview.showVegaPreview",
                "title": "Show Vega Preview"
            },
            {
                "command": "vega-preview.showVegaLitePreview",
                "title": "Show Vega-Lite Preview"
            },
            {
                "command": "vega-preview.exportVegaToSvg",
                "title": "Export Vega to Svg"
            },
            {
                "command": "vega-preview.exportVegaLiteToSvg",
                "title": "Export Vega-Lite to Svg"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "vega": "^4.0.0-rc.2",
        "vega-lite": "^2.5.2"
    }
}
