{
    "name": "vega-lite-preview",
    "displayName": "Vega-Lite Preview",
    "description": "Preview for Vega-Lite",
    "version": "0.0.1",
    "publisher": "mdk",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vega-lite.showPreview"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "vega-lite.showPreview",
                "title": "Show Vega-Lite Preview"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "vega": "^4.0.0-rc.2"
    }
}
